<mjml>

  <mj-head>

    <mj-attributes>
      <mj-text align="center" color="#000" />
      <mj-section background-color="#fff" />
      <mj-all font-family="Noto Sans, Arial"/> 
    </mj-attributes>
    
    <mj-font name="Noto Sans" href="https://fonts.googleapis.com/css?family=Noto+Sans:400,400i,700,700i"> </mj-font>
    <mj-font name="Rubik"  href="https://fonts.googleapis.com/css?family=Rubik:300,300i,400,400i,500,500i,700,700i,900,900i"> </mj-font>
    <mj-font name="Rationale"  href="https://fonts.googleapis.com/css?family=Rationale"> </mj-font>
    
    
    
    <!-- Inline Style -->
   
    <mj-style inline="inline">
      
      /*-------- Fonts --------*/
      
      @import url('https://fonts.googleapis.com/css?family=Rubik:300,300i,400,400i,500,500i,700,700i,900,900i');
      
      @import url('https://fonts.googleapis.com/css?family=Noto+Sans:400,400i,700,700i');
      
      @import url('https://fonts.googleapis.com/css?family=Rationale');
      
      
      /*-------- Text --------*/

      h1 {
        font-size: 32px;
        font-family: "Rationale", Arial, sans-serif;
        margin: 0 0 10px 0;
      }
      
      h2 {
        font-size: 20px;
      }
      
      h2, h3 {
        font-family: "Rubik", Arial, sans-serif;
        font-weight: bold;
      }
      
      h4 {
        font-size: 14px;
      }
      
      p {
        line-height: 16px;
        font-family: "Noto Sans", Arial, sans-serif;
      }
      
      a {
        text-decoration: none;
        color: black;
      }
      
      
      /*-------- Main section --------*/

      
      .band {
        font-weight: 700;
        font-size: 16px;
      }
      
      .fornever {
        color:#e04b00;
      }
      
      .banner_date {
        margin: 0 0 20px 0;
      }
      
      .google {
        font-weight: bold;
      }
      
      
      /*-------- Artist section --------*/
      
      .block-title {
      	background-color: #212a34;
      	padding: 0 15px;
        display: inline-block;
        text-align: center;
        margin: 30px 0;
      }
      
      .block_link {
        color:white;
        margin: 0;
      }
      

      .title-container {
      	color: white;
      	text-align: center;
      	background-image: url("http://res.cloudinary.com/ducdzjqsh/image/upload/v1526978367/line4.png");
      	background-repeat: repeat-x;
      	background-position: center;
        margin: 0 20px;
      }
      
      .artist-container{
        min-height: 300px;
        height: 100%;
        background: url("http://res.cloudinary.com/ducdzjqsh/image/upload/v1526378538/Portrait-Dface2-AO-http-_www.timhans.com_-copie.jpg") center/cover no-repeat;
      }
      
      
      /*-------- Expo section --------*/
      
      .expo {
        background-color: #fff;
        padding: 0 15px;
        display: inline-block;
      }
      
      .expo_container {
        text-align: center;
        background-image: url("http://res.cloudinary.com/ducdzjqsh/image/upload/v1526991554/line_expo1.png");
        background-repeat: repeat-x;
        background-position: center;
      }
      
      /*-------- "VU" section --------*/
      
      
      .vu {
        transform: rotate(-4deg);
      }
      
      .vudansle, .face {
        background-color: #212a34;
      }
      
      .graph-container{
        min-height: 250px;
        height: 100%;
        background: url("http://res.cloudinary.com/ducdzjqsh/image/upload/v1526978979/pantonio_graph.jpg") center/cover no-repeat;
      }
      
      /*-------- "Footer" --------*/
      
      .hidden {
        color: rgba(255,255,255,0);
      }
      
    </mj-style>
    
    
    <!-- Hover non diponible en ligne -->

    <mj-style>
    a.google:hover {
      color: #e04b00 !important;
     }
      
    .block_link:hover {
      text-decoration: underline !important;
      }  
    </mj-style>
    
  </mj-head>
  
  
  
